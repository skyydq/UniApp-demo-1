<view><view class="search"><input class="search-input" type="text" focus placeholder="请输入要搜索的联系人" data-event-opts="{{[['input',[['handleInput',['$event']]]]]}}" bindinput="__e"/></view><block wx:if="{{keyword}}"><view class="search-main"><block wx:if="{{hasNoData}}"><view class="search-main-errtitle">无搜索结果</view></block><block wx:for="{{list}}" wx:for-item="item" wx:for-index="__i0__" wx:key="id"><view class="search-main-title" hover-class="hover" hover-start-time="{{20}}" hover-stay-time="{{70}}" data-name="{{item.name}}" data-id="{{item.id}}" data-phoneNumber="{{item.phoneNumber}}" data-event-opts="{{[['tap',[['handleClick',['$event']]]]]}}" bindtap="__e">{{''+item.name+''}}</view></block></view></block></view>