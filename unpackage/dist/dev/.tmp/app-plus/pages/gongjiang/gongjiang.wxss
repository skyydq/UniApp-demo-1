	height: 100rpx;
	line-height: 100rpx;
	text-align: center;
}
.uni-popup-bottom {
	left: 0;
	bottom: 0;
	width: 100%;
	min-height: 100rpx;
	line-height: 100rpx;
	text-align: center;
}

